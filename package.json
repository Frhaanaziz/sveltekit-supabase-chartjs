{
	"name": "sveltekit-supabase-dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"version": "node -p \"require('./package.json').version\"",
		"check-versions": "npm run version && npm run build",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-types": "npx supabase gen types typescript --project-id yaczmlasurwnhxorzovo > src/types/supabase.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.44.0",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.30.4",
		"@tailwindcss/typography": "^0.5.13",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^2.52.0",
		"dayjs": "^1.11.11",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "^8.4.38",
		"postcss-load-config": "^4.0.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.17",
		"svelte-check": "^3.7.1",
		"svelte-feather-icons": "^4.1.0",
		"svelte-loading-spinners": "^0.3.6",
		"svelte-time": "^0.7.2",
		"tailwind-scrollbar": "^2.1.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^4.5.3",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.9.4",
		"@supabase/supabase-js": "^2.43.2",
		"chart.js": "^4.4.3",
		"chartjs-adapter-date-fns": "^3.0.0",
		"date-fns": "^2.30.0",
		"svelte-chartjs": "^3.1.5",
		"svelte-french-toast": "^1.2.0",
		"svelte-markdown": "^0.3.0",
		"svelte-select": "^5.8.3",
		"svelte-table": "^0.5.11",
		"theme-change": "^2.5.0"
	}
}
