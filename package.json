{
	"name": "sveltekit-supabase-dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"version": "node -p \"require('./package.json').version\"",
		"check-versions": "npm run version && npm run build",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"update-types": "npx supabase gen types typescript --project-id yaczmlasurwnhxorzovo > src/types/supabase.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.44.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.5.9",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/typography": "^0.5.13",
		"@typescript-eslint/eslint-plugin": "^7.10.0",
		"@typescript-eslint/parser": "^7.10.0",
		"autoprefixer": "^10.4.19",
		"daisyui": "^4.11.1",
		"eslint": "^9.3.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "^8.4.38",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"svelte": "^4.2.17",
		"svelte-check": "^3.7.1",
		"svelte-feather-icons": "^4.1.0",
		"svelte-loading-spinners": "^0.3.6",
		"sveltekit-superforms": "^2.13.1",
		"tailwind-scrollbar": "^3.1.0",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.13.0",
		"@supabase/supabase-js": "^2.43.2",
		"chart.js": "^4.4.3",
		"chartjs-adapter-date-fns": "^3.0.0",
		"dotenv": "^16.4.5",
		"svelte-chartjs": "^3.1.5",
		"svelte-french-toast": "^1.2.0",
		"svelte-markdown": "^0.4.1",
		"svelte-table": "^0.6.3",
		"theme-change": "^2.5.0"
	}
}